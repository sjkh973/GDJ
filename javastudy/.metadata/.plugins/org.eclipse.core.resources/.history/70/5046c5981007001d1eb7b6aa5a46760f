package ex03_number;

public class Ex01 {
	
	public static void main(String[] args) {
		
		// 산술 연산
		
		int a = 7;
		int b = 2;
		int result1 = a+b;
		int result2 = a-b;
		int result3= a*b;
		int result4 = a/b;
		int result5 = a%b;
		
		System.out.println(result1);
		System.out.println(result2);
		System.out.println(result3);
		System.out.println(result4);
		System.out.println(result5);
		
	}
}
